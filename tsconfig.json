{
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "strict": true,
    "noImplicitAny": false,
    "noImplicitThis": false,
    "alwaysStrict": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "jsx": "react-jsx",
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "skipLibCheck": true,
    "types": [],
    "noEmit": false,
    "declaration": true,
    "declarationMap": false,
    "outDir": "./build",
    "removeComments": false,
    "stripInternal": true,
    "emitDeclarationOnly": true
  },
  "include": [
    "tsconfig.d.ts",
    "src"
  ],
  "exclude": [
    "node_modules"
  ],
  "typeAcquisition": {
    "include": [
      "react",
      "react-dom"
    ]
  }
}
